{
  "name": "generator-alfresco",
  "version": "0.0.1",
  "description": "Yeoman generator",
  "license": "Apache-2.0",
  "main": "app/index.js",
  "repository": "binduwavell/generator-alfresco",
  "author": {
    "name": "Bindu Wavell",
    "email": "bindu@ziaconsulting.com",
    "url": "https://github.com/binduwavell"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "generators/app"
  ],
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "yeoman-generator": "0.22.2",
    "chalk": "1.1.1",
    "cross-spawn": "2.1.0",
    "deasync": "0.1.4",
    "eyes": "0.1.8",
    "lodash": "3.10.1",
    "pretty-data": "0.40.0",
    "rmdir": "1.1.0",
    "semver": "5.1.0",
    "split": "1.0.0",
    "walk": "2.3.9",
    "xmldom": "0.1.19",
    "xpath": "0.0.9",
    "yosay": "1.1.0"
  },
  "devDependencies": {
    "coveralls": "2.11.6",
    "fixme": "0.3.0",
    "istanbul": "0.4.1",
    "mem-fs": "COMMENT: this should be kept in sync with yeoman-environment, from yeoman-generator",
    "mem-fs": "1.1.2",
    "mem-fs-editor": "COMMENT: this should be kept in sync with yeoman-generator",
    "mem-fs-editor": "2.1.0",
    "mocha": "2.3.4",
    "mocha-lcov-reporter": "1.0.0",
    "yeoman-assert": "2.1.1",
    "yeoman-test": "1.0.0"
  },
  "scripts": {
    "fixme": "node -e \"require('fixme')()\"",
    "test": "mocha --reporter spec test -w",
    "test:only": "cd test; mocha --reporter spec",
    "test:single": "mocha --reporter spec test",
    "howto-cover-local": "echo 'Place a repo_token: entry with the project secret from the coveralls dashboard into .coveralls.yml' ; echo 'WARNING: do not commit this secret!!!'",
    "cover": "istanbul cover _mocha -- --report spec test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cover:local": "istanbul cover _mocha -- --report spec test ; node -e \"require('fixme')()\""
  }
}
