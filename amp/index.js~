'use strict';
var yeoman = require('yeoman-generator');

module.exports = yeoman.generators.Base.extend({
  initializing: function () {
    this.log('You called the Alfresco AMP subgenerator with the argument.');
  },

  writing: function () {

        this.fs.copyTpl(
          this.templatePath('projects-pom.xml'),
          this.destinationPath('project/pom.xml'),
          {groupId : "com.ampgen",
           version : "1.0-SNAPSHOT",
           projectName : "amp-gen-test"}
        );   
      
  }
});
